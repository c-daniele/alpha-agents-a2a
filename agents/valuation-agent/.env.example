# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here

# LangSmith Tracing Configuration
LANGSMITH_API_KEY=your_langsmith_api_key_here
LANGSMITH_PROJECT=alpha-agents-valuation
LANGSMITH_TRACING=true

# LLM Provider Configuration
LLM_PROVIDER=openai  # Options: openai, bedrock
LLM_MODEL=gpt-4o    # For OpenAI: gpt-4o, gpt-4o-mini | For Bedrock: claude-sonnet-4-20250514-v1:0

# AWS Bedrock Configuration (only if LLM_PROVIDER=bedrock)
AWS_PROFILE=sbox_admin
AWS_REGION=us-east-1

# Server Configuration
HOST=0.0.0.0
PORT=3001

# Agent Configuration
AGENT_NAME=valuation-agent
AGENT_VERSION=1.0.0
LOG_LEVEL=INFO

# Analysis Configuration
DEFAULT_ANALYSIS_PERIOD_DAYS=365
DEFAULT_RISK_FREE_RATE=0.05
TRADING_DAYS_PER_YEAR=252